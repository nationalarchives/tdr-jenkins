library("tdr-jenkinslib")

def versionTag = "v${env.BUILD_NUMBER}"
def repo = "tdr-consignment-export"

pipeline {
  agent {
    ecs {
      inheritFrom 'transfer-frontend'
    }
  }

  stages {
    stage('Test') {
      agent {
        ecs {
          inheritFrom 'transfer-frontend'
        }
      }
      steps {
        checkout scm
        sh 'plugin-updates'
      }
    }
  }
}
